<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Qualisoft APP | {% endblock %}</title>

    <!-- Bootstrap core CSS -->
    {% block stylesheets %}
    <link href="{{ asset('bundles/qualisoftapp/css/bootstrap.min.css') }}" rel="stylesheet">

    <link href="{{ asset('bundles/qualisoftapp/fonts/css/font-awesome.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/qualisoftapp/css/animate.min.css') }}" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="{{ asset('bundles/qualisoftapp/css/custom.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/qualisoftapp/css/icheck/flat/green.css') }}" rel="stylesheet">
    {% endblock %}

    {% block header_javascripts %}
    <script src="{{ asset('bundles/qualisoftapp/js/jquery.min.js') }}"></script>
    {% endblock %}
    

    {% block IE_javascripts %}
    <!--[if lt IE 9]>
        <script src="../assets/js/ie8-responsive-file-warning.js"></script>
        <![endif]-->

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}    

</head>


<body class="nav-md">

    <div class="container body">


        <div class="main_container">

            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">

                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Qualisoft.CO!</span></a>
                    </div>
                    <div class="clearfix"></div>


                    <!-- menu prile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img src="images/img.jpg" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Hola,</span>
                            <h2>Diego Torres</h2>
                        </div>
                    </div>
                    <!-- /menu prile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                        {% block sidebar %}    
                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li><a><i class="fa fa-home"></i> Home <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li><a href="index.html">Escritorio 1</a>
                                        </li>
                                        <li><a href="index.html">Escritorio 2</a>
                                        </li>
                                        <li><a href="index3.html">Escritorio 3</a>
                                        </li>
                                    </ul>
                                </li>
                                <!-- @diegotorres50: copiar el bloque <li><a><i class="fa fa-xxx"> para agregar mas sub menus, aqui la clase 'fa-home'
                                se usa para personalizar el icono del modulo.
                                //-->
                            </ul>
                        </div>
                        <!-- @diegotorres50 para otros bloques de menu categorizados, duplique <div class="menu_section"> y
                        entre <h3> indique el nombre de ese bloques de menus.
                        //-->
                        {% endblock %}

                    </div>
                    <!-- /sidebar menu -->

                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        {% block footerbuttons %}
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                        {% endblock %}
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                {% block top_nav %}
                <div class="nav_menu">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img src="images/img.jpg" alt="">Diego Torres
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu animated fadeInDown pull-right">
                                    <li><a href="javascript:;">  Profile</a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">
                                            <span class="badge bg-red pull-right">50%</span>
                                            <span>Settings</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">Help</a>
                                    </li>
                                    <li><a href="login.html"><i class="fa fa-sign-out pull-right"></i> Log Out</a>
                                    </li>
                                </ul>
                            </li>

                            <!-- modulo general de alertas de usuario //-->
                            <li role="presentation" class="dropdown">
                                <!-- icono de sobre  //-->
                                <a href="javascript:;" class="dropdown-toggle info-number" data-toggle="dropdown" aria-expanded="false">
                                    <i class="fa fa-envelope-o"></i>
                                    <span class="badge bg-green">6</span>
                                </a>
                                <!-- /icono de sobre //-->
                                <!-- modulo de alertas en fila //-->
                                <ul id="menu1" class="dropdown-menu list-unstyled msg_list animated fadeInDown" role="menu">
                                    <li>
                                        <a>
                                            <span class="image">
                                        <img src="images/img.jpg" alt="Profile Image" />
                                    </span>
                                            <span>
                                        <span>John Smith</span>
                                            <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                        Film festivals used to be do-or-die moments for movie makers. They were where... 
                                    </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a>
                                            <span class="image">
                                        <img src="images/img.jpg" alt="Profile Image" />
                                    </span>
                                            <span>
                                        <span>John Smith</span>
                                            <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                        Film festivals used to be do-or-die moments for movie makers. They were where... 
                                    </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a>
                                            <span class="image">
                                        <img src="images/img.jpg" alt="Profile Image" />
                                    </span>
                                            <span>
                                        <span>John Smith</span>
                                            <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                        Film festivals used to be do-or-die moments for movie makers. They were where... 
                                    </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a>
                                            <span class="image">
                                        <img src="images/img.jpg" alt="Profile Image" />
                                    </span>
                                            <span>
                                        <span>John Smith</span>
                                            <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                        Film festivals used to be do-or-die moments for movie makers. They were where... 
                                    </span>
                                        </a>
                                    </li>
                                    <li>
                                        <div class="text-center">
                                            <a>
                                                <strong>See All Alerts</strong>
                                                <i class="fa fa-angle-right"></i>
                                            </a>
                                        </div>
                                    </li>
                                </ul>
                                <!-- /modulo de alertas en fila //-->
                            </li>
                            <!-- /modulo general de alertas de usuario //-->
                        </ul>

                    </nav>
                </div>
                {% endblock %}

            </div>
            <!-- /top navigation -->

            <!-- page content -->
            <div class="right_col" role="main">

                <div class="">
                    <div class="page-title">
                        <!-- bloque del titulo principal //-->
                        <div class="title_left">
                            <h3>{% block module_title %}T&iacute;tulo principal {% endblock %}</h3>
                        </div>
                        <!-- /bloque del titulo principal //-->

                        <!-- buscador -->
                        <div class="title_right">
                            {% block title_right %}
                            <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Buscar...">
                                    <span class="input-group-btn">
                                        <button class="btn btn-default" type="button">Ir!</button>
                                    </span>
                                </div>
                            </div>
                            {% endblock %}
                        </div>
                        <!-- /buscador -->

                    </div>
                    <div class="clearfix"></div>

                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <!-- bloque del subtitulo //-->
                                <div class="x_title">
                                    {% block x_title %}
                                    <h2>{% block module_subtitle %}Subt&iacute;tulo{% endblock %}</h2>
                                    <ul class="nav navbar-right panel_toolbox">
                                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                            <ul class="dropdown-menu" role="menu">
                                                <li><a href="#">Settings 1</a>
                                                </li>
                                                <li><a href="#">Settings 2</a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li><a class="close-link"><i class="fa fa-close"></i></a>
                                        </li>
                                    </ul>
                                    <div class="clearfix"></div>
                                    {% endblock %}
                                </div>
                                <!-- /bloque del subtitulo //-->


                                <div class="x_content">
                                    {% block x_content %}

                                    {% block content_lead %}<p>Aqui va un lead  .</p>{% endblock %}
                                    <!-- contenido principal aqui -->

                                    <!-- /contenido principal aqui -->
                                    {% endblock %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- footer content -->
                <footer>
                    {% block footer %}
                    <div class="">
                        <p class="pull-right">Qualisoft.CO! una aplicaci&oacute;n de <a>diegotorres.info</a>. |
                            <span class="lead"> <i class="fa fa-paw"></i> Qualisoft.CO!</span>
                        </p>
                    </div>
                    <div class="clearfix"></div>
                    {% endblock %}
                </footer>
                <!-- /footer content -->

            </div>
            <!-- /page content -->
        </div>

    </div>

    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group">
        </ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>

    {% block footer_javascripts %}
    <script src="{{ asset('bundles/qualisoftapp/js/bootstrap.min.js') }}"></script>

    <!-- chart js //-->
    <script src="{{ asset('bundles/qualisoftapp/js/chartjs/chart.min.js') }}"></script> 
    <!-- bootstrap progress js //--> 
    <script src="{{ asset('bundles/qualisoftapp/js/progressbar/bootstrap-progressbar.min.js') }}"></script> 
    <script src="{{ asset('bundles/qualisoftapp/js/nicescroll/jquery.nicescroll.min.js') }}"></script> 
    <!-- icheck //-->
    <script src="{{ asset('bundles/qualisoftapp/js/icheck/icheck.min.js') }}"></script> 

    <script src="{{ asset('bundles/qualisoftapp/js/custom.js') }}"></script>
    <!-- dropzone //-->
    <script src="{{ asset('bundles/qualisoftapp/js/dropzone/dropzone.js') }}"></script>
    {% endblock %} 

</body>

</html>